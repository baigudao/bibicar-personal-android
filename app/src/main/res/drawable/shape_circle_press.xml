<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- center circle -->
    <item
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp">
        <shape android:shape="oval">
            <solid android:color="@color/car_color_14" />
            <size android:height="5dp" android:width="5dp"/>
        </shape>
    </item>

    <!-- outer circle -->
    <item>
        <shape
            android:layout_width="wrap_content"
            android:shape="oval">
            <stroke
                android:width="1dp"
                android:color="@color/car_color_14"/>
        </shape>
    </item>



</layer-list>